:root{
    --lightColor: #ddd;
    --darkColor: #333;
}

.main {
    color: red;
}

.red {
    color: red;
}

.warp {
    width: 100px;
    height: 100px;
}

::placeholder {
    color: red;
}

@custom-selector :--heading h1, h2, h3, h4, h5, h6;

@custom-selector :--enter :focus, :hover;

a:--enter {
    color: #f40;
}

:--heading {
    font-weight: bold;
}

:--heading.active {
    font-weight: bold;
}





